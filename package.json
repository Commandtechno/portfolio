{
  "scripts": {
    "build": "npm run build:js && npm run build:css && npm run build:html",
    "build:js": "esbuild ./js/index.ts --bundle | ts-node --transpile-only swc.ts",
    "build:css": "esbuild ./css/index.css --bundle --minify --outfile=./build/index.css",
    "build:html": "ts-node --transpile-only cheerio.ts"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.168",
    "@types/node": "^17.0.24",
    "esbuild": "^0.14.36",
    "ts-node": "^10.7.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10"
  }
}